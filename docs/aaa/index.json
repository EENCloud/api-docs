{
    "apiVersion": "1.0.0",
    "swaggerVersion": "1.2",
    "basePath": "https://22222.eagleeyenetworks.com/g",
    "resourcePath": "/aaa",
    "produces": ["application/json"],
    "apis": [
        {
            "path": "/aaa/authenticate",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Login Step 1: Authenticate with user credentials",
                    "notes": "Logins are a 2 step process:  Authenticate, then Authorize with the token returned by Authenticate.",
                    "nickname": "authenticate",
                    "type": "AuthenticateToken",
                    "parameters": [
                        {
                            "name": "username",
                            "description": "Username",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        },
                        {
                            "name": "password",
                            "description": "Password",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        },
                        {
                            "name": "realm",
                            "description": "Realm",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "User has been authenticated. Body contains JSON encoded result"
                        },
                        {
                            "code": 400,
                            "message": "Some argument(s) are missing or invalid"
                        },
                        {
                            "code": 401,
                            "message": "Supplied credentials are not valid"
                        },
                        {
                            "code": 402,
                            "message": "Account is suspended"
                        },
                        {
                            "code": 412,
                            "message": "User is disabled"
                        },
                        {
                            "code": 460,
                            "message": "Account is inactive"
                        },
                        {
                            "code": 461,
                            "message": "Account is pending"
                        },
                        {
                            "code": 462,
                            "message": "User is pending. This will be thrown before 401 if username is valid and Account is active."
                        }
                    ]
                }
            ]
        },
        {
            "path": "/aaa/authorize",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Login Step 2: Authorize user with token",
                    "notes": "Authorize is the second step of the Login process, by using the token from the first step (Authenticate). This returns an authorized user object.",
                    "nickname": "authorize",
                    "type": "AuthorizeUser",
                    "parameters": [
                        {
                            "name": "token",
                            "description": "Token returned by authenticate",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "User has been authorized for access to the realm"
                        },
                        {
                            "code": 400,
                            "message": "Some argument(s) are missing or invalid"
                        },
                        {
                            "code": 401,
                            "message": "Invalid Token supplied"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/aaa/logout",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Log out",
                    "notes": "Log out user and invalidate HTTP session cookie",
                    "nickname": "logout",
                    "parameters": [],
                    "responseMessages": [
                        {
                            "code": 204,
                            "message": "User has been logged out"
                        }
                    ]
                }
            ]
        }
    ],
    "models":{
        "AuthenticateToken":{
            "id":"AuthenticateToken",
            "required":[],
            "properties":{
                "token":{
                    "type":"string",
                    "description":"Authentication Token needed for aaa/authorize",
                    "required": true
                }
            }
        },
        "AuthorizeUser":{
            "id":"AuthorizeUser",
            "required":[],
            "properties":{
                "id":{
                    "type":"string",
                    "description":"Unique identifier for Authorized User",
                    "required": true
                },
                "user_id": {
                    "type":"string",
                    "description":"Unique identifier for Authorized User (same as 'id')",
                    "required": true
                },
                "first_name": {
                    "type":"string",
                    "description":"First name of Authorized User",
                    "required": true
                },
                "last_name": {
                    "type":"string",
                    "description":"Last name of Authorized User",
                    "required": true
                },
                "email": {
                    "type":"string",
                    "description":"Email of Authorized User",
                    "required": true
                },
                "owner_account_id": {
                    "type":"string",
                    "description":"Unique identifier of user's Account",
                    "required": true
                },
                "active_account_id": {
                    "type":"string",
                    "description":"Unique identifier of user's active Account",
                    "required": true
                },
                "uid": {
                    "type":"string",
                    "description":"UID",
                    "required": false
                },
                "is_superuser": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user a Super User",
                    "required": false
                },
                "is_account_superuser": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user an Account Super User",
                    "required": false
                },
                "is_staff": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user a Staff User",
                    "required": false
                },
                "is_active": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user Active",
                    "required": false
                },
                "is_pending": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user a Pending user",
                    "required": false
                },
                "is_master": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user in a Master Account",
                    "required": false
                },
                "is_user_admin": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user a User Admin",
                    "required": false
                },
                "is_layout_admin": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user a Layout Admin",
                    "required": false
                },
                "is_live_video": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user authorized to access Live Video",
                    "required": false
                },
                "is_device_admin": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user a Device Admin",
                    "required": false
                },
                "is_export_video": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user authorized to Export Video",
                    "required": false
                },
                "is_recorded_video": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is the user authorized to view Recorded Video",
                    "required": false
                },
                "street": {
                    "type":"array",
                    "description":"Street Address as array [address line 1, address line 2, ...])",
                    "required": false
                },
                "city": {
                    "type":"string",
                    "description":"City",
                    "required": false
                },
                "state": {
                    "type":"string",
                    "description":"State",
                    "required": false
                },
                "country": {
                    "type":"string",
                    "description":"Country",
                    "required": false
                },
                "postal_code": {
                    "type":"string",
                    "description":"Postal Code",
                    "required": false
                },
                "phone": {
                    "type":"string",
                    "description":"Phone number",
                    "required": false
                },
                "mobile_phone": {
                    "type":"string",
                    "description":"Mobile phone number",
                    "required": false
                },
                "utc_offset": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Timezone offset from UTC in seconds (signed integer)",
                    "required": false
                },
                "timezone": {
                    "type":"string",
                    "description":"Timezone",
                    "required": false
                },
                "last_login": {
                    "type":"string",
                    "description":"Last time the user logged in, in EEN timestamp format: YYYYMMDDHHMMSS.NNN",
                    "required": false
                },
                "alternate_email": {
                    "type":"string",
                    "description":"Alternate email address",
                    "required": false
                },
                "sms_phone": {
                    "type":"string",
                    "description":"SMS phone number",
                    "required": false
                },
                "is_sms_include_picture": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Include picture in sms notifications",
                    "required": false
                },
                "json":{
                    "type":"string",
                    "description":"Misc settings for the user as a JSON string",
                    "required": false
                },
                "camera_access": {
                    "type":"array",
                    "description":"List of devices (IDs) the user has access to",
                    "required": false
                },
                "layouts": {
                    "type":"array",
                    "description":"List of layouts (IDs) the user has access to",
                    "required": false
                },
                "is_notify_enable": {
                    "type":"integer",
                    "format":"int64",
                    "description":"Is notifications enabled for the User",
                    "required": false
                },
                "notify_period": {
                    "type":"array",
                    "description":"List of notification time periods, in the form: D-HHMM-HHMM",
                    "required": false
                },
                "notify_rule": {
                    "type":"array",
                    "description":"List of notification rules, in the form: id-type-delay (e.g. one-email-0)",
                    "required": false
                },
                "access_period": {
                    "type":"array",
                    "description":"List of access time periods, in the form: D-HHMM-HHMM",
                    "required": false
                }
            }
        }
    }
}
{
    "apiVersion": "1.0.0",
    "swaggerVersion": "1.2",
    "basePath": "https://22222.eagleeyenetworks.com/g",
    "resourcePath": "/aaa",
    "produces": ["application/json"],
    "apis": [
        {
            "path": "/aaa/authenticate",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Authenticate",
                    "notes": "Returns an auth token",
                    "nickname": "authenticate",
                    "type": "string",
                    "parameters": [
                        {
                            "name": "username",
                            "description": "Username",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        },
                        {
                            "name": "password",
                            "description": "Password",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        },
                        {
                            "name": "realm",
                            "description": "Realm",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "User has been authenticated. Body contains JSON encoded result"
                        },
                        {
                            "code": 400,
                            "message": "Some argument(s) are missing or invalid"
                        },
                        {
                            "code": 401,
                            "message": "Supplied credentials are not valid"
                        },
                        {
                            "code": 402,
                            "message": "Account is suspended"
                        },
                        {
                            "code": 412,
                            "message": "User is disabled"
                        },
                        {
                            "code": 460,
                            "message": "Account is inactive"
                        },
                        {
                            "code": 461,
                            "message": "Account is pending"
                        },
                        {
                            "code": 462,
                            "message": "User is pending. This will be thrown before 401 if username is valid and Account is active."
                        }
                    ]
                }
            ]
        },
        {
            "path": "/aaa/authorize",
            "operations": [
                {
                    "method": "POST",
                    "summary": "Authorize",
                    "notes": "Returns authorized user",
                    "nickname": "authorize",
                    "parameters": [
                        {
                            "name": "token",
                            "description": "Token",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "User has been authorized for access to the realm"
                        },
                        {
                            "code": 400,
                            "message": "Some argument(s) are missing or invalid"
                        },
                        {
                            "code": 401,
                            "message": "Invalid Token supplied"
                        }
                    ]
                }
            ]
        }
    ],
    "models": {}
}
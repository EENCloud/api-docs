                    "parameters": [
                        {
                            "name": "subject",
                            "description": "Subject of the feedback",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        },
                        {
                            "name": "message",
                            "description": "Feedback message",
                            "required": true,
                            "type": "string",
                            "paramType": "form"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 202,
                            "message": "Request succeeded"
                        },
                        {
                            "code": 400,

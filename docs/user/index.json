{
    "apiVersion": "1.0.0",
    "swaggerVersion": "1.2",
    "basePath": "https://22222.eagleeyenetworks.com/g",
    "resourcePath": "/user",
    "produces": ["application/json"],
    "apis": [
        {
            "path": "/user",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get User",
                    "notes": "Returns user object by ID. Not passing an ID will return the current authorized user.",
                    "nickname": "getUser",
                    "type": "User",
                    "parameters": [
                        {
                            "name": "id",
                            "description": "User ID",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "Request succeeded"
                        },
                        {
                            "code": 400,
                            "message": "Unexpected or non-identifiable arguments are supplied"
                        },
                        {
                            "code": 401,
                            "message": "Unauthorized due to invalid session cookie"
                        },
                        {
                            "code": 403,
                            "message": "Forbidden due to the user missing the necessary privileges"
                        }
                    ]
                },
                {
                    "method": "DELETE",
                    "summary": "Delete User",
                    "notes": "Delete a user",
                    "nickname": "deleteUser",
                    "parameters": [
                        {
                            "name": "id",
                            "description": "User ID",
                            "required": true,
                            "type": "string",
                            "paramType": "query"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "Delete succeeded"
                        },
                        {
                            "code": 400,
                            "message": "Unexpected or non-identifiable arguments are supplied"
                        },
                        {
                            "code": 401,
                            "message": "Unauthorized due to invalid session cookie"
                        },
                        {
                            "code": 403,
                            "message": "Forbidden due to the user missing the necessary privileges"
                        },
                        {
                            "code": 404,
                            "message": "User not found with the supplied ID"
                        }
                    ]
                }
            ]
        }
    ],

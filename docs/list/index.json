{
    "apiVersion": "1.0.0",
    "swaggerVersion": "1.2",
    "basePath": "https://22222.eagleeyenetworks.com/g",
    "resourcePath": "/list",
    "produces": ["application/json"],
    "apis": [
        {
            "path": "/list/devices",
            "operations": [
                {
                    "method": "GET",
                    "summary": "List Devices",
                    "notes": "Returns array of all devices available to the user",
                    "nickname": "listDevices",
                    "type": "array",
                    "items": {
                        "$ref": "ListDevice"
                    },
                    "parameters": [
                        {
                            "name": "e",
                            "description": "Device ID",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        },
                        {
                            "name": "n",
                            "description": "Device Name",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        },
                        {
                            "name": "t",
                            "description": "Device Type",
                            "required": false,
                            "type": "string",
                            "paramType": "query",
                            "enum": [
                                "camera",
                                "bridge"
                            ]
                        },
                        {
                            "name": "s",
                            "description": "Device Service Status",
                            "required": false,
                            "type": "string",
                            "paramType": "query",
                            "enum": [
                                "ATTD",
                                "IDLE",
                                "IGND",
                                "ERRR"
                            ]
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "Request succeeded"
                        },
                        {
                            "code": 400,
                            "message": "Unexpected or non-identifiable arguments are supplied"
                        },
                        {
                            "code": 401,
                            "message": "Unauthorized due to invalid session cookie"
                        },
                        {
                            "code": 403,
                            "message": "Forbidden due to the user missing the necessary privileges"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/list/accounts",
            "operations": [
                {
                    "method": "GET",
                    "summary": "List Accounts",
                    "notes": "Returns array of all accounts available to the user",
                    "nickname": "listAccounts",
                    "type": "array",
                    "items": {
                        "$ref": "ListAccount"
                    },
                    "parameters": [],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "Request succeeded"
                        },
                        {
                            "code": 400,
                            "message": "Unexpected or non-identifiable arguments are supplied"
                        },
                        {
                            "code": 401,
                            "message": "Unauthorized due to invalid session cookie"
                        },
                        {
                            "code": 403,
                            "message": "Forbidden due to the user missing the necessary privileges"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/list/users",
            "operations": [
                {
                    "method": "GET",
                    "summary": "List Users",
                    "notes": "Returns array of all users available to the user",
                    "nickname": "listUsers",
                    "type": "array",
                    "items": {
                        "$ref": "ListUser"
                    },
                    "parameters": [],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "Request succeeded"
                        },
                        {
                            "code": 400,
                            "message": "Unexpected or non-identifiable arguments are supplied"
                        },
                        {
                            "code": 401,
                            "message": "Unauthorized due to invalid session cookie"
                        },
                        {
                            "code": 403,
                            "message": "Forbidden due to the user missing the necessary privileges"
                        }
                    ]
                }
            ]
        }
    ],
    "models":{
        "ListUser":{
            "id":"ListUser",
            "required":[],
            "properties":{
                "id":{
                    "type":"string",
                    "description":"Unique identifier for the User"
                },
                "first_name":{
                    "type":"string",
                    "description":"First Name of the user"
                },
                "last_name":{
                    "type":"string",
                    "description":"Last Name of the user"
                },
                "email":{
                    "type":"string",
                    "description":"Email address of the user"
                },
                "permissions":{
                    "type":"array",
                    "description":"List of permissions the user has",
                    "items":{
                        "type":"string"
                    }
                },
                "last_login":{
                    "type":"string",
                    "description":"Last time the user logged in, in EEN timestamp format: YYYYMMDDHHMMSS.NNN"
                }
            }
        },
        "ListAccount":{
            "id":"ListAccount",
            "required":[],
            "properties":{
                "id":{
                    "type":"string",
                    "description":"Unique identifier for the Account"
                },
                "name":{
                    "type":"string",
                    "description":"Name of the account"
                }
            }
        },
        "ListDevice":{
            "id":"ListDevice",
            "required":[],
            "properties":{
                "account_id":{
                    "type":"string",
                    "description":"Unique identifier for the Device's Account"
                },
                "id":{
                    "type":"string",
                    "description":"Unique identifier for the Device"
                },
                "name":{
                    "type":"string",
                    "description":"Name of the device"
                },
                "type":{
                    "type":"string",
                    "description":"Type of device",
                    "enum": [
                        "camera",
                        "bridge"
                    ]
                },
                "bridges":{
                    "type":"array",
                    "description":"List of bridges that see the camera",
                    "items":{
                        "type":"string"
                    }
                },
                "service_status":{
                    "type":"string",
                    "description":"Device service status",
                    "enum": [
                        "ATTD",
                        "IDLE",
                        "IGND",
                        "ERRR"
                    ]
                },
                "permissions":{
                    "type":"string",
                    "description":"Permissions the user has on the device"
                },
                "tags":{
                    "type":"array",
                    "description":"Tags",
                    "items":{
                        "type":"string"
                    }
                },
                "guid":{
                    "type":"string",
                    "description":"GUID"
                },
                "serial_number":{
                    "type":"string",
                    "description":"Serial number"
                },
                "device_status":{
                    "type":"integer",
                    "format":"int64",
                    "description":"Device status bit mask"
                },
                "timezone":{
                    "type":"string",
                    "description":"Timezone"
                },
                "timezone_utc_offset":{
                    "type":"integer",
                    "format":"int64",
                    "description": "Timezone UTC offset as signed integer in seconds, such as “-25200”, which translates to -7 hours from UTC."
                },
                "is_unsupported":{
                    "type":"integer",
                    "format":"int64",
                    "description":"Indicates the camera is NOT supported (1) or IS supported (0)"
                },
                "ip_address":{
                    "type":"string",
                    "description":"IP Address of device"
                },
                "is_shared":{
                    "type":"integer",
                    "format":"int64",
                    "description":"Indicates the camera is shared (1) or not (0)"
                },
                "owner_account_name":{
                    "type":"string",
                    "description":"Name of the account that owns the device.  This only applies to shared cameras, since they will be owned by a different account."
                },
                "is_upnp":{
                    "type":"integer",
                    "format":"int64",
                    "description":"Indicates whether the camera is a UPNP device (1) or not (0).  Currently this only applies to cameras that haven’t yet been attached to the account, in which they could have been detected via ONVIF or UPNP."
                }
            }
        }
    }
}